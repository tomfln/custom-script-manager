{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "inputs": [
        "src/**",
        "!src/**/node_modules/**",
        "!src/**/target/**",
        "!src/**/dist/**",
        "templates/**",
        "*.ts",
        "*.json",
        "*.rs",
        "Cargo.toml",
        "pnpm-lock.yaml"
      ],
      "outputs": []
    }
  }
}
